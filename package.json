{
  "name": "@andybalham/sls-testing-toolkit",
  "version": "1.0.13",
  "description": "A toolkit for testing AWS serverless applications",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "eslint ./src --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test-coverage": "nyc npm run test",
    "all-synth": "tsc && cdk synth",
    "all-deploy": "tsc && cdk deploy",
    "all-destroy": "tsc && cdk destroy --all",
    "simple-event-router-synth": "tsc && cdk synth SimpleEventRouterTestStack",
    "simple-event-router-deploy": "tsc && cdk deploy SimpleEventRouterTestStack",
    "simple-event-router-destroy": "tsc && cdk destroy SimpleEventRouterTestStack",
    "simple-message-router-synth": "tsc && cdk synth SimpleMessageRouterTestStack",
    "simple-message-router-deploy": "tsc && cdk deploy SimpleMessageRouterTestStack",
    "simple-message-router-destroy": "tsc && cdk destroy SimpleMessageRouterTestStack",
    "notification-hub-synth": "tsc && cdk synth NotificationHubTestStack",
    "notification-hub-deploy": "tsc && cdk deploy NotificationHubTestStack",
    "notification-hub-destroy": "tsc && cdk destroy NotificationHubTestStack",
    "loan-processor-synth": "tsc && cdk synth LoanProcessorTestStack",
    "loan-processor-deploy": "tsc && cdk deploy LoanProcessorTestStack",
    "loan-processor-destroy": "tsc && cdk destroy LoanProcessorTestStack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/sls-testing-toolkit.git"
  },
  "author": "Andy Blackledge",
  "keywords": [
    "aws",
    "cdk",
    "serverless",
    "testing"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andybalham/sls-testing-toolkit/issues"
  },
  "homepage": "https://github.com/andybalham/sls-testing-toolkit#README",
  "devDependencies": {
    "@andybalham/state-machine-builder": "^1.0.11",
    "@andybalham/state-machine-with-graph": "^1.0.4",
    "@aws-cdk/aws-dynamodb": "^1.0.0",
    "@aws-cdk/aws-events": "^1.0.0",
    "@aws-cdk/aws-events-targets": "^1.0.0",
    "@aws-cdk/aws-lambda": "^1.0.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.0.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.0.0",
    "@aws-cdk/aws-sns": "^1.0.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.0.0",
    "@aws-cdk/aws-sqs": "^1.0.0",
    "@aws-cdk/aws-stepfunctions": "^1.0.0",
    "@aws-cdk/aws-stepfunctions-tasks": "^1.0.0",
    "@aws-cdk/core": "^1.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/aws-lambda": "^8.10.81",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "aws-sdk": "^2.958.0",
    "chai": "^4.3.4",
    "esbuild": "^0.12.17",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "esbuild": "0.x",
    "@aws-cdk/aws-dynamodb": "1.x",
    "@aws-cdk/aws-events": "1.x",
    "@aws-cdk/aws-events-targets": "1.x",
    "@aws-cdk/aws-lambda": "1.x",
    "@aws-cdk/aws-lambda-event-sources": "1.x",
    "@aws-cdk/aws-lambda-nodejs": "1.x",
    "@aws-cdk/aws-sns": "1.x",
    "@aws-cdk/aws-sns-subscriptions": "1.x",
    "@aws-cdk/aws-sqs": "1.x",
    "@aws-cdk/aws-stepfunctions": "1.x",
    "@aws-cdk/aws-stepfunctions-tasks": "1.x",
    "@aws-cdk/core": "1.x"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "nanoid": "^3.0.0"
  }
}
