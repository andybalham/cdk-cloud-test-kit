    "aws-cdk-lib": "^2.x",
    "constructs": "^10.x",
    "esbuild": "0.x"
  },
  "dependencies": {
    "aws-xray-sdk": "^3.5.0",
    "dotenv": "^10.0.0",
    "nanoid": "^3.0.0",
    "node-fetch": "^2.6.7"
  }
}
