{
  "mochaExplorer.files": "test/**/*.ts",
  "mochaExplorer.require": "ts-node/register",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.language": "en-GB",
  "cSpell.words": [
    "Chainable",
    "dlqs",
    "esbuild",
    "eventbridge",
    "exts",
    "hotswap",
    "istanbuljs",
    "Letsby",
    "nanoid",
    "nolookalikes",
    "Plodville",
    "resourcegroupstaggingapi",
    "stepfunctions",
    "TRes",
    "vars"
  ],
  "workbench.colorCustomizations": {
    "[Default Dark+]": {
      "titleBar.activeBackground": "#130049",
      "activityBar.background": "#130049",
      "activityBar.activeBackground": "#5a3bb1",
      "sideBar.background": "#0b0029"
    },
    "[Default Light+]": {
      "titleBar.activeBackground": "#7d4ffc",
      "activityBar.background": "#5f29f5",
      "activityBar.activeBackground": "#7d4ffc",
      "sideBar.background": "#dad1f1",
      "panel.background": "#dad1f1",
      "terminal.background": "#ebe8f3"
    }
  },
  "serverlessConsole.services": [
    {
      "type": "cloudformation",
      "title": "Request API",
      "timeOffsetInMs": 0,
      "awsProfile": "default",
      "stacks": [
        {
          "region": "eu-west-2",
          "stackName": "RequestApiTestStack",
          "stage": "dev"
        }
      ]
    }
  ]
}
